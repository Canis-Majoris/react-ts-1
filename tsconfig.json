{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noImplicitAny": false,
    "noEmit": false,
    "sourceMap": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@Components/*": ["./components/*"],
      "@Constants/*": ["./constants/*"],
      "@Types/*": ["./types/*"],
      "@Hooks/*": ["./hooks/*"],
      "@Modules/*": ["./modules/*"],
      "@Redux/*": ["./redux/*"],
      "@Styles/*": ["./styles/*"],
      "@Config/*": ["./config/*"],
      "@Assets/*": ["./assets/*"],
      "@Utils/*": ["./utils/*"],
      "@Providers/*": ["./providers/*"],
      "@Routes/*": ["./routes/*"],
      "@Queries/*": ["./queries/*"],
      "@Features/*": ["./features/*"],
      "@State/*": ["./state/*"]
    }
  },
  "include": ["src", "custom.d.ts"],
}
